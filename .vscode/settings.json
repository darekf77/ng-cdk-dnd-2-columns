{
    "workbench.colorCustomizations": {
        "activityBar.background": "#275d91",
        "activityBar.foreground": "#000000",
        "statusBar.background": "#275d91"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.rulers": [
        80,
        120,
        140
    ],
    "search.exclude": {
        "**/.git": true,
        "**/bower_components": true,
        "**/vendor": true,
        "**/external": true,
        "**/node_modules": true
